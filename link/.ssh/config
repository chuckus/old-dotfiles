Host *

# Forward ssh agent to the remote machine.
ControlMaster auto
ControlPersist yes

Host debook
  ForwardX11Trusted yes
  ForwardX11 yes
  ForwardAgent yes

Host tv
  HostName home.means.no
  User tv
  
Host mtv
  ForwardAgent yes
  HostName home.means.no

Host beast
    ProxyCommand ssh nordaaker.com -W %h:%p
    User root

Host cloudia 
    ProxyCommand ssh nordaaker.com -W %h:%p
    User root
    RequestTTY yes
Host stagolee
    ProxyCommand ssh nordaaker.com -W %h:%p
    RequestTTY yes
    User root
Host fatsie
    ProxyCommand ssh nordaaker.com -W %h:%p
    User root
Host raker
    ProxyCommand ssh nordaaker.com -W %h:%p
    User root
Host wirc 
    ProxyCommand ssh nordaaker.com -W %h:%p
    User root
Host output 
    ProxyCommand ssh nordaaker.com -W %h:%p
    User root
